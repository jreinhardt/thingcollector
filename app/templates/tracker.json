{
   "version":0.0,
   "id" : {{ config.TRACKER_UUID }},
   "url" : {{ config.TRACKER_URL }},
   "thingsCount" : 0,
   "trackersCount" : {{ trackers|length }},
   "trackersTraversalDepth" : 1,
   "maintainers" : [
      {
         "url" :  {{ config.MAINTAINER_EMAIL }},
         "name" : {{ config.MAINTAINER_NAME }}
      }
   ],
   "things":[],
   "trackers":[
      {% for tracker in trackers %}
      {
         "url" : {{ tracker.url }}
      }
      {% endfor %}
   ]
}
